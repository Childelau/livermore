.btn{
    display: flex;
    flex-direction: row;
    align-items: stretch;
    justify-content: space-around;
    background-color: #fff;
    height:100rpx;
    font-size: 32rpx;
    border-top:1px solid #F0F0F0;
    border-bottom:1px solid #F0F0F0;
}
.btn>view{
    display: flex;
    align-items: center;
}
.active{
  color: #1296db;
  border-bottom:3px solid #1296db;
}
.swiper-box{
  height: 86%;
}
page{
  height: 100%;
}
.swiper-box scroll-view{
  height: 100%;
}

.swiper-box .title{
  text-align: center;
  padding: 1rem 0;
  font-size: 20px;
  text-decoration: underline;
}
.fcontent{
  padding: 0 1rem;
}
.red{
  color: red;
  display: inline;
}
.fcontent>view{
  margin-bottom: 0.6rem;
}
.fcontent image{
  width: 100%;
}















