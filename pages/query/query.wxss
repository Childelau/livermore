.title1{
  height: 3rem;
  text-align: center;
  line-height: 3rem;
  color: white;
  font-size: 18px;
  background-color: #2B91D5;
  letter-spacing: 3px;
}
.title2{
  padding: 5px 0 10px 16px;
  font-size: 14px;
  font-weight: bold;
  border-bottom: 1px lightgrey solid;
}
.title3{
  padding: 16px 16px 30px 16px;
}
.title3 input{
  border: 1px lightgray solid;
  padding: 8px;
}
.title4{
  padding: 0 16px;
}
.title4 view{
  width: 100%;
  background-color: #2B91D5;
  color: white;
  height: 2.6rem;
  text-align: center;
  line-height: 2.6rem;
  font-weight: bold;
  border-radius: 6px;
  letter-spacing: 2px;
}


























